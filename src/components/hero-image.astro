---
import { Picture } from "@astrojs/image/components";
import serverImage from "~/assets/servers.webp";

const widths = [450, 800, 1200];
const sizes = "100vw";
---

<Picture
  class="object-cover w-full h-full"
  src={serverImage}
  widths={widths}
  sizes={sizes}
  alt="The servers in the datacenter"
/>
